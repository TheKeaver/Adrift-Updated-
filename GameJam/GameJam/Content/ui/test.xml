<UI>
    <Widgets>
        <DropDownPanel
            halign="center"
            valign="top"
            horizontal="0"
            vertical="10%"
            width="50%"
            height="50%"
            aspect-ratio="20:4"
            released-image="texture_ui_button_released"
            released-thickness="5,5,5,5"
            hover-image="texture_ui_button_hover"
            hover-thickness="5,5,5,5"
            pressed-image="texture_ui_button_pressed"
            pressed-thickness="5,5,5,5"
            close-on="UI.TestButtonPressedEvent,UI.TestButtonPressed2Event"
            defaultSelected="true"
            alpha="0.5"
            >
            <Label
                halign="left"
                valign="center"
                horizontal="5%"
                vertical="0"
                width="50%"
                height="50%"

                content="DropDown"
                font="font_game_over"
                />
            <Contents
                width="100%"
                height="300%">
                <Image
                    halign="center"
                    valign="center"
                    horizontal="0"
                    vertical="0"
                    width="100%"
                    height="100%"
                    
                    image="texture_ui_dropdown_background"
                    />
                <Button
                    halign="center"
                    valign="top"
                    horizontal="0"
                    vertical="0"
                    width="100%"
                    height="33.333333%"
                    released-image="texture_ui_button_released"
                    released-thickness="5,5,5,5"
                    hover-image="texture_ui_button_hover"
                    hover-thickness="5,5,5,5"
                    pressed-image="texture_ui_button_pressed"
                    pressed-thickness="5,5,5,5"
                    onclick="queue:UI.TestButtonPressedEvent"
                    id="dropdown-1-button-1"
                    aboveID="dropdown-1-button-3"
                    belowID="dropdown-1-button-2"
                    />
                <Button
                    halign="center"
                    valign="top"
                    horizontal="0"
                    vertical="33.333333%"
                    width="100%"
                    height="33.333333%"
                    released-image="texture_ui_button_released"
                    released-thickness="5,5,5,5"
                    hover-image="texture_ui_button_hover"
                    hover-thickness="5,5,5,5"
                    pressed-image="texture_ui_button_pressed"
                    pressed-thickness="5,5,5,5"
                    onclick="queue:UI.TestButtonPressed2Event"
                    id="dropdown-1-button-2"
                    aboveID="dropdown-1-button-1"
                    belowID="dropdown-1-button-3"
                    />
                <Button
                    halign="center"
                    valign="top"
                    horizontal="0"
                    vertical="66.666666%"
                    width="100%"
                    height="33.333333%"
                    released-image="texture_ui_button_released"
                    released-thickness="5,5,5,5"
                    hover-image="texture_ui_button_hover"
                    hover-thickness="5,5,5,5"
                    pressed-image="texture_ui_button_pressed"
                    pressed-thickness="5,5,5,5"
                    onclick="queue:UI.TestButtonPressedEvent"
                    id="dropdown-1-button-3"
                    aboveID="dropdown-1-button-2"
                    belowID="dropdown-1-button-1"
                    />
            </Contents>
        </DropDownPanel>
